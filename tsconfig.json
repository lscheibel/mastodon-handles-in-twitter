{
    "compilerOptions": {
        "target": "es2022",
        "useDefineForClassFields": true,
        "lib": ["DOM", "ESNext"],
        "allowJs": false,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": false,
        "noEmit": false,
        "jsx": "react-jsx"
    },
}
